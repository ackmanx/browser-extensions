{
  "name": "Video Killer",
  "description": "Kill all video",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>",
    "webNavigation"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Kill!",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
